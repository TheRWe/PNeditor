{
  "name": "pnet_editor",
  "productName": "pnet_editor",
  "version": "1.0.0",
  "description": "Electron.PNetEditor",
  "main": "./js_compiled/electron/program.js",
  "license": "MIT",
  "scripts": {
    "electron": "electron .",
    "start": "yarn compile && electron .",
    "script": "cd ./scripts/ && ts-node-script",
    "clean": "yarn script clean",
    "compile":"tsc && yarn script build",
    "build": "yarn compile && electron-builder",
    "lint": "tslint -p tslint.json",
    "lint:fix": "yarn tslint -- --fix"
  },
  "keywords": [
    "Electron",
    "Pnet",
    "editor"
  ],
  "author": "Wehmhoner Roman",
  "dependencies": {
    "d3": "5.7.0",
    "object-hash": "^1.3.1",
    "ts-keycode-enum": "1.0.6"
  },
  "devDependencies": {
    "@types/d3": "5.7.0",
    "@types/node": "^13.1.8",
    "@types/object-hash": "^1.3.0",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.0",
    "pkg": "^4.4.8",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  },
  "build": {
    "appId": "PetriNetEditor",
    "win": {
      "target": "portable",
      "icon": "./src/PNIcon.png"
    },
    "linux": {
      "target": "deb",
      "icon": "./src/PNIcon.png"
    }
  }
}
